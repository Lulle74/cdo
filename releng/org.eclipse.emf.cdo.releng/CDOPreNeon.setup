      </predicate>
    </workingSet>
    <workingSet
        name="Net4j Examples"
        id="CDO Net4j Examples">
      <predicate
          xsi:type="predicates:AndPredicate">
        <operand
            xsi:type="predicates:OrPredicate">
          <operand
